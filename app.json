{
	"expo": {
		"name": "Pet Find",
		"description": "Uma aplicação que o vai ajudar a encontrar o seu animal de estimação!",
		"slug": "pet_find",
		"privacy": "public",
		"sdkVersion": "36.0.0",
		"platforms": ["ios", "android", "web"],
		"version": "2.0.0",
		"orientation": "portrait",
		"icon": "./assets/paw_icon.png",
		"splash": {
			"image": "./assets/paw_transparent.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"notification": {
			"icon": "./assets/paw_transparent_96_96.png"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"config": {
				"googleMobileAdsAppId": "${GOOGLE_MOBILE_ADS_APP_ID}"
			}
		},
		"android": {
			"package": "pt.fhenriques.petfind",
			"versionCode": 5,
			"config": {
				"googleMobileAdsAppId": "${GOOGLE_MOBILE_ADS_APP_ID}",
				"googleMaps": {
					"apiKey": "${GOOGLE_MAPS_API_KEY}"
				}
			}
		},
		"androidStatusBar": {
			"backgroundColor": "#ffffff",
			"barStyle": "dark-content"
		}
	}
}
